{
  "heading": {
    "xxl": { "sizeVar": "--font-size-heading-xxl", "lineHeight": "1.1", "weight": "var(--font-weight-bold)", "letterSpacing": "var(--font-letter-spacing-tight)" },
    "xl":  { "sizeVar": "--font-size-heading-xl",  "lineHeight": "1.15", "weight": "var(--font-weight-semibold)", "letterSpacing": "var(--font-letter-spacing-wide)" },
    "l":   { "sizeVar": "--font-size-heading-l",   "lineHeight": "1.2", "weight": "var(--font-weight-semibold)", "letterSpacing": "var(--font-letter-spacing-wide)" },
    "m":   { "sizeVar": "--font-size-heading-m",   "lineHeight": "1.25", "weight": "var(--font-weight-semibold)", "letterSpacing": "var(--font-letter-spacing-wide)" },
    "s":   { "sizeVar": "--font-size-heading-s",   "lineHeight": "1.3", "weight": "var(--font-weight-semibold)", "letterSpacing": "var(--font-letter-spacing-wide)" },
    "xs":  { "sizeVar": "--font-size-heading-xs",  "lineHeight": "1.4", "weight": "var(--font-weight-semibold)", "letterSpacing": "var(--font-letter-spacing-wide)" }
  },
  "body": {
    "l":  { "sizeVar": "--font-size-body-l",  "lineHeight": "var(--font-line-height-relaxed)" },
    "m":  { "sizeVar": "--font-size-body-m",  "lineHeight": "1.5" },
    "s":  { "sizeVar": "--font-size-body-s",  "lineHeight": "1.4" },
    "xs": { "sizeVar": "--font-size-body-xs", "lineHeight": "1.3" }
  },
  "semantics": {
    "display": "xxl",
    "headline": "xl",
    "title": "l",
    "body": "m",
    "label": "xs"
  }
}
